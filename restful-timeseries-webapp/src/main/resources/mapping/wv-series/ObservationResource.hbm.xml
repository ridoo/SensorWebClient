<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.api.v1.da.beans">
	<class name="ObservationEntity" table="observation">
		<id name="pkid" type="long">
			<column name="observation_id" />
			<generator class="assigned">
			</generator>
		</id>
		
		<property name="seriesPkid" column="series_pkid"  type="long" />
		<property name="timestamp" column="time_stamp" type="timestamp" />
		<property name="value" column="numeric_value" type="double" />
        
	</class>
	
</hibernate-mapping>
